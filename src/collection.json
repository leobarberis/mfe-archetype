{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "mfe": {
      "description": "MFE Generation",
      "factory": "./mfe-archetype/new-mfe#newMfe",
      "schema": "./mfe-archetype/schema.json"
    },
    "new-container": {
      "description": "Container Generation",
      "factory": "./mfe-archetype/new-container#newContainer"
    },    
    "build-container": {
      "description": "Container Dev Generation",
      "factory": "./mfe-archetype/index#buildContainer"
    },
    "add-mfe": {
      "description": "Add MFE to Container",
      "factory": "./mfe-archetype/add-mfe#addMFE",
      "schema": "./mfe-archetype/schema-add-mfe.json"
    },     
    "delete-mfe": {
      "description": "Delete MFE from Container",
      "factory": "./mfe-archetype/delete-mfe#deleteMFE",
      "schema": "./mfe-archetype/schema-delete-mfe.json"
    }
  }
}
